{
  "functions": {
    "public/*.php": {
      "runtime": "vercel-php@0.6.0"
    },
    "includes/**/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.php"
    },
    {
      "src": "/login",
      "dest": "/public/login.php"
    },
    {
      "src": "/register",
      "dest": "/public/register.php"
    },
    {
      "src": "/dashboard",
      "dest": "/public/dashboard.php"
    },
    {
      "src": "/tasks",
      "dest": "/public/tasks.php"
    },
    {
      "src": "/calendar",
      "dest": "/public/calendar.php"
    },
    {
      "src": "/motivation",
      "dest": "/public/motivation.php"
    },
    {
      "src": "/reports",
      "dest": "/public/reports.php"
    },
    {
      "src": "/logout",
      "dest": "/public/logout.php"
    },
    {
      "src": "/email-test",
      "dest": "/public/email_test.php"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "PHP_VERSION": "8.1"
  },
  "build": {
    "env": {
      "PHP_VERSION": "8.1"
    }
  }
}
